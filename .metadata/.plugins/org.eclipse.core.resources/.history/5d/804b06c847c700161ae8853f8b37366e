			(function(d, t) {
				var r = d.createElement(t),
					s = d.getElementsByTagName(t)[0];
				r.async = 1;
				r.src = hosturl + '/js/new/home.js?' + (new Date()).getTime().toString();
				s.parentNode.insertBefore(r, s);
			})(document, "script");
		}
		function isLogin() {
			var login_sn = UserInfo.has_login();
			if (login_sn) {
				var contact = UserInfo.contactName();
				$(".havelogin").show();
				$(".nologin").hide();
				$(".qiandaomain").show();
				//$(".my-name").text("您好，" + contact + "！")
			} else {
				$(".havelogin").hide();
				$(".nologin").show();
				$(".qiandaomain").hide();
				//$(".my-name").text("请登录")
			}
		}

		//签到
		function hideqiandao(){
			$(".qiandaobtn").hide();
			$(".haveqiandao").show();
		}
		var haveqiandao=false
		function qiandao() {
			if (haveqiandao==true){
				return false;
			}
			haveqiandao=true
			var data = {
				company_id : UserInfo.memberID(),
				usertoken : UserInfo.token(),
				appsystem : api.systemType,
				clientid : api.deviceId,
				datatype : "json",
			}
			api.ajax({
				url : hosturl + 'qianbao/qiandao.html',
				method : 'get',
				timeout : 30,
				dataType : 'json',
				returnAll : false,
				data : {
					values : data
				}
			}, function(ret, err) {
				if (ret) {
					if (ret.err == "false") {
						api.toast({
							msg : '签到成功！再生钱包  + 0.5元',
							duration : 2000,
							location : 'middle'
						});
						ajaxInfo();
						$(".qiandaobtn").hide();
						$(".haveqiandao").show();
						api.execScript({
							name : 'root',
							frameName : 'index-frame',
							script : 'hideqiandao()'
						});
					} else {
						api.toast({
							msg : '你今天已经签到',
							duration : 2000,
							location : 'middle'
						});
					}
					$api.rmStorage("indexdata");
				} else {
					if(err){
						saveerrlog(err.body);
					}
					api.toast({
						msg : '系统错误',
						duration : 2000,
						location : 'middle'
					});
				};
				haveqiandao=false;
			});
		}
		
	</script>
