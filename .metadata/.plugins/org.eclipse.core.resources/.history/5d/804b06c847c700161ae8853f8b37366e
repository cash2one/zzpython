	<script type="text/javascript">
		var mobilevalue="";
		var companyname="";
		function showquestionnum(num){
			//我的留言提示数字
			if (num==0){
				$(".questioncount").hide();
				api.execScript({
					name : 'root',
					script : "hidemainmyhome()"
				});
			}else{
				$(".questioncount").show();
				$(".questioncount").text(num);
			}
		}
		//显示，修改头像
		function showfacehtml(faceurl){
			if (faceurl){
				var facehtml="<img src='"+faceurl+"'>";
			}else{
				var facehtml="<img src='../../image/noavatar.gif'>"
			}
			$("#faceurl").html(facehtml)
		}
		//重新显示登陆
		function relogin(){
			$(".havelogin").hide();
			$(".nologin").show();
			$("#loading").hide();
			$(".qiandaomain").hide();
			showfacehtml();
		}
		var n=0;
		function ajaxInfo(){
			var login_sn = UserInfo.has_login();
			
			if (login_sn){
				
				$(".havelogin").show();
				$(".nologin").hide();
				var data={
					company_id : UserInfo.memberID(),
					usertoken : UserInfo.token(),
					appsystem : api.systemType,
					clientid : api.deviceId,
					datatype : "json",
				}
				//alert(UserInfo.token())
				api.ajax({
					url : hosturl +"myrc_index/",
					method : "get",
					timeout : 30,
					dataType : 'json',
					returnAll : false,
					data : {
						values : data
					}
				}, function(ret, err) {
					if (ret){
						if (ret.hasOwnProperty("tokenerr")){
							n=n+1;
							if (n>=5){
								relogin();
							}else{
								if (ret.tokenerr=='true'){
									tokeninfo();
									ajaxInfo()
								}
							}
							return;
						}
						if (ret.err=="true"){
							zzalert(ret.errkey);
							relogin();
							return;
						}
						//未完善公司信息提示完善
						if (ret.completeflag){
							if (ret.completeflag.showflag==1){
								$(".completediv").show();
								$(".zxinfo").css("margin-top","1px");
								$(".showtext").html(ret.completeflag.text);
								$(".nowanshan").show();
							}else{
								$(".completediv").hide();
								$(".zxinfo").css("margin-top","-15px");
								$(".nowanshan").hide();
							}
						}
						//是否企业秀
						if (ret.isqiyexiu){
							$(".qiyexiu").show();
						}else{
							$(".qiyexiu").hide();
						}
						companyname=ret.companyname;
						//诚信认证
						if (!ret.zxinfo){
							$(".zxinfo").show();
							$(".norenzheng").show();
						}else{
							$(".zxinfo").hide();
							$(".norenzheng").show();
							if (ret.zxinfo=='0'){
								$(".norenzheng").html("未审核")
							}
							if (ret.zxinfo=='2'){
								$(".norenzheng").html("审核未通过")
							}
							if (ret.zxinfo=='1'){
								$(".norenzheng").hide();
							}
						}
						if(ret.ldbvalue){
							//设置来电宝客户类型
							$api.setStorage("ldbvalue", 1);
							$(".ldbmain").show();
							$(".ldbnav").show();
							$(".qiandaomain").hide();
							$(".nomalnav").hide();
							$(".my-name").html("您好，" + ret.contact + "！")
							$(".blance").text(ret.blance);
							$(".ldbtel").text("您的来电宝电话："+ret.ldbvalue.front_tel)
							$(".ldbjtl").text(ret.ldbjtl);
							mobilevalue=ret.mobile;
							var faceurl=ret.faceurl;
							showfacehtml(faceurl)
						}else{
							$api.setStorage("ldbvalue", 0);
							$(".ldbmain").hide();
							$(".ldbnav").hide();
							$(".qiandaomain").show();
							$(".nomalnav").show();
							$(".my-name").html("您好，" + ret.contact + "！")
							$(".blance").text(ret.blance);
							mobilevalue=ret.mobile;
							var faceurl=ret.faceurl;
							showfacehtml(faceurl)
							//我的留言提示数字
							if (ret.qcount==0){
								$(".questioncount").hide();
							}else{
								$(".questioncount").show();
								$(".questioncount").text(ret.qcount);
							}
							//签到
							if (ret.qiandao){
								$(".haveqiandao").show();
								$(".qiandaobtn").hide();
							}else{
								$(".haveqiandao").hide();
								$(".qiandaobtn").show();
							}
						}
						//商务助理
						if (ret.cslist){
							$(".csname").html(ret.cslist.name);
							$(".cstel").html("<a href='tel:"+ret.cslist.tel+"'>"+ret.cslist.tel+"</a>");
							$(".cszhuli").show();
						}
						
						
					}else{
						if (n<=20){
							ajaxInfo();
							n=n+1
						}else{
							relogin()
							return;
						}
						if(err){
							saveerrlog(err.body);
						}
					}
					$("#loading").hide();
					//zzalert(JSON.stringify(ret))
				});
			}else{
				relogin();
			}
		}
		apiready = function() {
			var closeiosflag=$api.getStorage("closeiosflag");
			if (!closeiosflag || closeiosflag==1){
				$(".versionupate").hide();
				var login_sn = UserInfo.has_login();
				if (!login_sn) {
					$(".mycontent1").hide();
					$(".mycontent2").hide();
				}else{
					$(".mycontent1").show();
					$(".mycontent2").show();
				}
			}
			
			tokeninfo();
			//var header = $api.byId('topbar');
			//$api.fixStatusBar(header);
			$(".version").html(api.appVersion);
			//如监听网络连接事件
			api.addEventListener({
			    name: 'online'
			}, function(ret, err){
			    if( ret ){
			    	ajaxInfo();
			    	api.hideProgress();
			    }else{
			    	
			    }
			});
			ajaxInfo();
			
			
			$(".loginout").on("click", function() {
				api.confirm({
					title : '提示',
					msg : '确实要退出吗？',
					buttons : ['取消', '确定']
				}, function(ret, err) {
					if (ret) {
						if (ret.buttonIndex == 2) {
							UserInfo.clear();
							ajaxInfo();
							api.toast({
								msg : '成功退出！',
								duration : 2000,
								location : 'bottom'
							});
							$(".qiandaomain").hide();
							$(".ldbmain").hide();
							$(".havelogin").hide();
							$(".nologin").show();
							$(".qiyexiu").hide();
						}
						//zzalert( JSON.stringify( ret ) );
					} else {
						//zzalert( JSON.stringify( err ) );
					}
				});
			})
			$(".login").on("click", function() {
				var login_sn = UserInfo.has_login();
				if (login_sn) {
					var pageParam = {
						wintitle : "修改资料",
						type : "pust-data",
						bounces : false
					};
					openWin("pust-data", "../myrc/pust-data.html", pageParam);
				} else {
					var pageParam = {
						wintitle : "登录",
						type : "frm-login",
						bounces : false
					};
					openWin("frm-login", "../myrc/frm-login.html", pageParam);
				}
			})
			$(".qiyexiu").on("click",".aui-btn", function() {
				var pageParam = {
					wintitle : "我的企业秀",
					type : "myqiyexiu",
					bounces : false
				};
				localStorage.setItem("share_title",""+companyname+"--ZZ91再生网企业秀");
				localStorage.setItem("share_description","");
				localStorage.setItem("share_url","http://h5.zz91.com/xiu/"+UserInfo.memberID().toString()+".html");
				localStorage.setItem("share_pic","http://img0.zz91.com/zz91/images/indexLogo.png");
				openWin("myqiyexiu", "http://h5.zz91.com/xiu/"+UserInfo.memberID()+".html", pageParam);
			})
			//签到
			$(".qiandaobtn").on("click", function() {
				//未登录
				if (!havelogin()) {
					return false;
				} else {
					qiandao();
				};
			})
			//来电宝充值
		    $(".ldbchongzhi").on("click",function(){
		        if (!UserInfo.has_login()) {
		        	var pageParam = {
						wintitle : "在线充值",
						type : "chongzhi",
						nextUrl : "../myrc/call-moery.html",
						winName : "chongzhi"
					};
					openWin("frm-login", "../myrc/frm-login.html", pageParam);
					return false;
		        }
		        var pageParam = {
		            wintitle:"在线充值",
		            type:"chongzhi",
		            paytype : "ldb",
		        };
		        var url="../myrc/call-moery.html"
		        //var url=hosturl+"qianbao/chongzhi_new.html?company_id="+UserInfo.memberID()+"&paytype=ldb"
		        openWin("chongzhi",url,pageParam);
		    });
			//我的名片
			$(".my-vcard").on("click", function() {
				/*
