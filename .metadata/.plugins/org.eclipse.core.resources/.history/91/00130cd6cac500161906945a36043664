	<script type="text/javascript">
		apiready = function() {
			$(".allservice").on("click","li", function() {
				if (!havelogin()) {
					return;
				}
				event.stopPropagation();
				var forcompany_id=$(this).attr("company_id");
				var index=$(this).attr("index");
				var wintitle=$(this).find(".aui-img-body").find("span").text();
				var pageParam = {
					wintitle : wintitle,
					type : "serviceshow"+index,
					bounces : false,
				};
				openWin("serviceshow"+index, "../service/show"+index+".html", pageParam);
			})
		}
		function showcontactcheck(valu) {
			$("#showcontactvalue").val(valu);
		}
		function backvaluecheck(valu) {
			$("#backvalue").val(valu);
		}

	</script>
