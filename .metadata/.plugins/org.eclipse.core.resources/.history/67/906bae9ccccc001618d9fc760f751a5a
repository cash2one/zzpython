	apiready = function(){
		var closeiosflag=$api.getStorage("closeiosflag");
		if (!closeiosflag || closeiosflag==1){
			$(".buybutton").hide();	
		}
		$(".buybutton").on("click", function() {
			//供求置顶
			event.stopPropagation();
			var pageParam = {
                wintitle:"关键字竞价排名",
                type:"servicejingjia",
                bounces:false
            };
            openWin("servicejingjia", "../service/show14_1.html", pageParam);
		})
	};
