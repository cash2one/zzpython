function getindexdata(ret){
	if (ret.adlistall) {
		var piclist = ret.adlistall;
		
		picpath=[],piccaptions=[],picurl=[];
		if (piclist){
			$.each(piclist, function(index, item) {
				if (item){
					picpath.push(item.picaddress);
					piccaptions.push(item.ad_title);
					picurl.push(item.target_url)
				}
			});
		}
		//未开启更新，关闭ios不能审核的功能
		var closeiosflag=$api.getStorage("closeiosflag");
		if (!closeiosflag || closeiosflag==1){
			$(".versionupate").hide();
			$(".midadlist").hide();
		}else{
			showscrollpic(picpath,piccaptions);
			$(".midadlist").show();
		}
	}		
	if (ret.qiandao=="1" || !ret.qiandao){
		$("#qiandaomain").hide();
	}else{
		$("#qiandaomain").show();
	}
	//行情报价
	var pageHtml = "";
	$.each(ret.hotnews, function(index, item) {
		var getTpl = $api.html($api.byId("hotnews-list"));
		laytpl(getTpl).render(item, function(html) {
			pageHtml = pageHtml + html;
		});
	})
	$(".hotnews").html(pageHtml);
	$(".procount").html(ret.pcount);
	$(".newsinfo").on("click",function(){
		var infoid = $(this).attr("id")
		var pageParam = {
          wintitle:"资讯详情",
          type:"detail",
          bounces:false,
          infoid:infoid
        };
        openWin("detail"+infoid.toString(),"../news/detail.html",pageParam);
	});
	
	api.hideProgress();
	api.refreshHeaderLoadDone();
	//认证提醒
	if (!ret.zxinfo && !ret.zxclose){
		//openwindows("gonggao",hosturl+"app/html/ad/gonggao.html");
		api.execScript({
			name : 'root',
			script : "opengonggao()"
		});
		layer.tips('立即分享给您的亲人和朋友吧。', '.box', {
			tips: [4, '#3595CC'],
			time: 6000
		});
	}
}

//打开搜索页面
function openSearch() {
	api.openWin({
		name : 'search',
		pageParam : api.pageParam,
		url : 'widget://html/comm/search.html',
		bounces : false,
		delay : 200
	});
}
//签到
function hideqiandao(){
	$("#qiandaomain").hide();
}
var haveqiandao=false;
function qiandao(){
	if (haveqiandao==true){
		return;
	}
	haveqiandao=true
	var data={
		company_id : UserInfo.memberID(),
	}
	api.ajax({
		url : hosturl + 'qianbao/qiandao.html?datatype=json',
		method : 'get',
		timeout : 30,
		dataType : 'json',
		returnAll : false,
		data : {
			values : data
		}
	}, function(ret, err) {
		if (ret) {
			if (ret.err=="false"){
				api.toast({
				    msg: '签到成功！再生钱包  + 0.5元',
				    duration: 2000,
				    location: 'middle'
				});
				$("#qiandaomain").hide();
				api.execScript({
					name : 'root',
					script : 'hideqiandao()'
				});
			}else{
				api.toast({
				    msg: '你今天已经签到',
				    duration: 2000,
				    location: 'middle'
				});
			}
			$api.rmStorage("indexdata");
		} else {
			if(err){
				saveerrlog(err.body);
			}
			api.toast({
			    msg: '系统错误',
			    duration: 2000,
			    location: 'middle'
			});
		};
		haveqiandao=false;
	});
}
//没人抢购的倒计时
function show_time() {
	var time_start = new Date().getTime();
	//设定当前时间
	var time_end = new Date("2016/10/11 00:00:00").getTime();
	//设定目标时间
	// 计算时间差
	var time_distance = time_end - time_start;
	// 天
	var int_day = Math.floor(time_distance / 86400000)
	time_distance -= int_day * 86400000;
	// 时
	var int_hour = Math.floor(time_distance / 3600000)
	time_distance -= int_hour * 3600000;
	// 分
	var int_minute = Math.floor(time_distance / 60000)
	time_distance -= int_minute * 60000;
	// 秒
	var int_second = Math.floor(time_distance / 1000)
	// 时分秒为单数时、前面加零
	if (int_day < 10) {
		int_day = "0" + int_day;
	}
	if (int_hour < 10) {
		int_hour = "0" + int_hour;
	}
	if (int_minute < 10) {
		int_minute = "0" + int_minute;
	}
	if (int_second < 10) {
		int_second = "0" + int_second;
	}
	var lastTime = int_day + "天" + int_hour + "小时" + int_minute + "分" + int_second + "秒";
	$(".j_time").html(lastTime);
	// 设置定时器
	setTimeout("show_time()", 1000);
}


