//顶部滚动图片
function showscrollpic(picpath,piccaptions) {
	var sysType = api.systemType;
	var pic_top=$(".mainitem").height()+$("#topbar").height()+20;
	var UIScrollPromptView = api.require('UIScrollPromptView');
	UIScrollPromptView.open({
	    rect: {
	        x: 0,
	        y: pic_top,
	        w: api.winWidth,
	        h: 50
	    },
	    data: {
	        paths: picpath,
	        
	    },
	    styles: {
	        
	        indicator: {
	            align: 'center',
	            color: '#FFFFFF',
	            activeColor: '#666'
	        },
	        
	    },
	    placeholderImg: 'widget://image/ad_load.png',
	    contentMode: 'scaleToFill',
	    interval: 6,
	    fixedOn: api.frameName,
	    loop: true,
	    auto:true,
	    fixed: false
	},function( ret ){
	    if( ret ){
	         if (ret.eventType=="click"){
				var pageParam = {
					wintitle : "广告",
					type : "adlist",
					bounces : false
				};
				var tourl = picurl[ret.index];
				openWin("adlist", tourl, pageParam);
			}
	    }
	});
	
	
}