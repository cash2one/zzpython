<script>
$(document).ready(function() {
	var timera=null;
	var i=0;
	var w=960;
	var len=$(".zwinfo ul>li").length;
	$(".zwinfo ul").css("width",960*len);
	//alert($(".zwinfo ul").css("width"))
	$(".prov").click(function(){
		var leftnum=w*i;
		if (i<len){
			$(".zwinfo ul").stop().animate({left:"-"+leftnum.toString()+"px"});
			i+=1;
		}
		if (i>len){i=0;}
	});
	$(".prov").mouseover(function(){
		clearTimeout(timera);
	});
	$(".prov").mouseout(function(){
		timera=setTimeout(louautoMove,1000);
	});
	
	$(".nextp").mouseover(function(){
		clearTimeout(timera);
	});
	$(".nextp").mouseout(function(){
		timera=setTimeout(louautoMove,1000);
	});
	
	$(".nextp").click(function(){
		if (i>1){
			i=i-1;
			var leftnum=w*(i-1);
			$(".zwinfo ul").stop().animate({left:"-"+leftnum.toString()+"px"});
			if (i<0){i=len-1;}
		}
	});
	function autogoup(id){
		var leftnum=id*w;
		$(".zwinfo ul").stop().animate({left:"-"+leftnum.toString()+"px"});
	}
	function louautoMove(){
			 autogoup(i);
			 timera=setTimeout(louautoMove,3000);
			 i=i+1;
			 if (i>=len){i=0}
	 }
	 $(".zwinfo ul").mouseover(function()  //移动到上面时停止自动切换
	{
		clearTimeout(timera);
	});
	$(".zwinfo ul").mouseout(function()  //移开时继续自动切换
	{
		timera=setTimeout(louautoMove,1000);
	});
	 timera=setTimeout(louautoMove,100);
});

</script>
