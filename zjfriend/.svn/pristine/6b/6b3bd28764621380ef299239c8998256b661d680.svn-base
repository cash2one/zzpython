body{
	background-color:#fff;
}
.mui-bar-nav {
	background-color: #fff;
	color: #fff;
}
.mui-bar-nav a, .mui-title {
	color: #fff;
}
.o_main {
	margin: 10px;
	color: #333;
	background-color:#fff;
}
.o_main ul {
	padding: 0;
	margin: 0;
}
.o_main li {
	float: left;
	line-height: 30px;
	height:30px;
	margin-top:20px;
	list-style: none;
}
.o_item {
	width: 70%;
}
.o_itemright {
	width: 33%;
	text-align: center;
}