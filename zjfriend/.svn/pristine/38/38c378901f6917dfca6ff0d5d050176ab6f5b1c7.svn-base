#suc_div{
	-moz-border-radius: 6px;
	background: #e0f0fa;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #536376;
	-webkit-box-shadow: rgba(0,0,0,.6) 0px 2px 12px;
	-moz-box-shadow:  rgba(0,0,0,.6) 0px 2px 12px;
	width: 60%;
	padding:15px;
	position:relative;
	display: none;
}
.suc_qx{width:100%;cursor:pointer}
.suc_qx img{float:right}
.suc_txt{text-align:center;color:#009944;font-size:20px;margin-top:15px}

#fail_div{
	-moz-border-radius: 6px;
	background: #e0f0fa;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	border: 1px solid #536376;
	-webkit-box-shadow: rgba(0,0,0,.6) 0px 2px 12px;
	-moz-box-shadow:  rgba(0,0,0,.6) 0px 2px 12px;
	width: 60%;
	padding:15px;
	position:relative;
	display: none;
}

.fail_txt{text-align:center;color:#f60000;font-size:20px;margin-top:15px}
.fail_tel{line-height:20px;font-size:14px;text-align:center}


.midload{position:fixed;left:50%;top:50%;width:250px;height:100px;margin-left:-125px;margin-top:-100px;color:#999}

/*分享*/
.sharehref{
	width:80%;
	-webkit-user-select:text;
	border: 1px solid #6C6C6C;
	-webkit-border-radius: 2px;
	border-radius: 2px;
	margin-bottom: .5em;
}
.popover{
	display:none;
	position:fixed;
	width:100%;
	height:100%;
	top:0;
	text-align: center;
	background-color: rgba(0,0,0,0.8);
	overflow: hidden;
	opacity:0;
	-webkit-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	z-index:99998;
}
.menu{
	display:none;
	position:fixed;
	left:1%;
	width:98%;
	background: #FFF;
	bottom:55px;
	border-radius: 2px;
	opacity:0;
	-webkit-transform:translateY(100%);
	-webkit-transition: all 0.3s ease-in;
	transition: all 0.3s ease-in;
	z-index:99999;
}
.button {
	font-size: 18px;
	font-weight: normal;
	text-decoration: none;
	display: block;
	text-align: center;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	color: #FFF;
	background-color: #00AA00;
	border: 1px solid #006600;
	padding: .5em 0em;
	margin: .5em .7em;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}
.button:active {
	outline: 0;
  	-webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
	box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
}
.button-waring {
	color: #666;
	background-color: #ebebeb;
	border-color: #e0e0e0;
}
.button-select {
	font-size: 14px;
	background-color: #CCCCCC;
	border: 0;
	-webkit-border-radius: 2px;
	border-radius: 2px;
}