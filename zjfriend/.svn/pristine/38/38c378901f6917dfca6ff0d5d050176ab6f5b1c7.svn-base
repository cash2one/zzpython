html, body, .mui-content {background-color: #efeff4;font-family: "微软雅黑";font-size: 100%;}
ul,li{list-style:none;padding: 0px;margin: 0px;font-size: 14px;}
a{color: #222222;}
hr{color: #ebebeb;}
.mui-content > .mui-table-view:first-child {margin-top: 1px;}
.zz91-index-num{position: absolute; top: 5px; left: 45px;}
.message-num{position: absolute; left: 38%;z-index: 999;}
.zz91-index-title{color: #0062CC;}
.zz91-question-main{padding: 10px;}
.zz91-question-main h2{font-size: 16px;color: #0062CC;line-height: 30px;;}
.zz91-question-left{text-align: left;float: left;clear: both;}
.zz91-question-right{text-align: right;float: right;clear: both;}
.zz91-question-anwser-grad{position: relative; background: #ccc; padding: 10px; color: #333; margin-bottom: 20px;border-radius:5px;margin-top: 15px;}
.zz91-question-arrow-grad{position: absolute; left: 30px; top: -16px; width: 0; height: 0; font-size: 0; border-width: 16px; border-style: dashed dashed dashed solid; border-color: transparent transparent transparent #ccc;}
.zz91-question-anwser-greed{position: relative; background: #2AC845; padding: 10px; color: #fff; margin-bottom: 20px;border-radius:5px;margin-top: 15px;}
.zz91-question-arrow-greed{position: absolute; right:40px; top: -16px; width: 0; height: 0; font-size: 0; border-width: 16px; border-style:dashed solid dashed dashed; border-color:transparent #2AC845 transparent transparent ;}
.zz91-question-time{color: #999;font-size: 14px;}
.zz91-question-addpic{float: left;width: 10%;text-align: center;line-height: 30px;}
.zz91-question-addtext{float: left;width: 65%;margin-right: 5px;margin-top: 5px;}
.zz91-question-addsend{width: 20%;line-height: 25px;}
.zz91-offerlist-bars{list-style: none;padding: 0px;margin: 0px;clear: both;border-top:solid 1px #ebebeb; width: 100%;}
.zz91-offerlist-bars li{float: left;text-align: center;width: 25%;line-height: 50px;background-color: #fff;border-right:solid 1px #f2f2f2;border-bottom:solid 1px #f2f2f2;position: relative;"}
.zz91-offerlist-bars span{width:10px;; height:0px; border:5px solid; border-color:#666 #ffffff #ffffff #ffffff;display: block;position: absolute;left: 73%;top: 45%;}
.pulldiv{line-height: 20px;text-align: center;color: #666666;font-size: 14px;width: 100%; background-color: #fff; margin-left:auto;margin-right:auto;height: 40px;padding-top: 5px;margin-top: 10px;}
.pulldiv .mui-spinner{float: left; margin-left: 30%;}
.pulldiv span{}
/*资讯中心*/
.news_detail{padding: 10px;font-size: 20px;}

.news_detail h4{text-align: left;line-height: 30px;font-size: 25px; margin-bottom: 10px;}
.news_detail .time{text-align: center;font-size: 12px;color: #777777;}
.news_detail .content{margin-top: 20px;line-height: 30px;font-size: 20px;color: #242424;}
.news_detail .content p{color: #000;font-size: 17px;}
.news_detail .content img{align-content: center;width:99%}
.news_detail .relevantnews{font-weight: bold;margin-top: 20px;line-height: 30px;font-size: 14px;}
.news_detail .rele_content{padding: 15px;margin: 0px;}
.news_detail .rele_content li{line-height: 2;}

/*交易中心*/
.tradelistnav {width: 100%;height100%;position: absolute;border: solid 1px #ebebeb;z-index: 9999;background-color: #fff;-webkit-box-shadow: #666 0px 0px 2px;-moz-box-shadow: #666 0px 0px 5px;box-shadow: #999 2px 2px 5px;}
.tradelistnav li{float: none;width: 100%;line-height: 3;border-collapse:collapse;}


.i-tab2{color: #000;}
