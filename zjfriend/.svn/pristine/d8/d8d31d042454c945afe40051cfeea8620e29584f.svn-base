<script>
apiready = function() {
	api.ajax({
		url : 'http://apptest.zz91.com/sex/navlist.html',
		method : 'get',
		timeout : 30,
		pageParam : {
			t : (new Date()).getTime().toString()
		},
		dataType : 'text',
		returnAll : false
	}, function(ret, err) {
		if (ret) {
			//alert(ret)
			$(".mui-content").html(ret);
			//api.hideProgress();
		} else {
			api.alert({
				//msg : ('错误码：' + err.code + '；错误信息：' + err.msg + '网络状态码：' + err.statusCode)
			});
		};
	});
};
